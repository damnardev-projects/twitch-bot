<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0"
      xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.damnardev.twitch.bot.primary.javafx.adapter.ApplicationController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Pane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" minWidth="200.0"
          VBox.vgrow="ALWAYS"/>
    <HBox maxWidth="1.7976931348623157E308">
        <Label alignment="BASELINE_RIGHT" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
               text="Channel Name:">
            <padding>
                <Insets right="10.0"/>
            </padding>
        </Label>
        <TextField maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"
                   fx:id="channelName"/>
    </HBox>
    <Button maxWidth="1.7976931348623157E308" onAction="#onAddChannelClick" text="Add Channel" textAlignment="CENTER">
    </Button>
    <Label fx:id="message" maxWidth="1.7976931348623157E308"/>
</VBox>
